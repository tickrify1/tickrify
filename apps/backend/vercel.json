{
  "functions": {
    "src/vercel.ts": {
      "maxDuration": 300,
      "memory": 1024,
      "includeFiles": [
        "prisma/**",
        "node_modules/.prisma/**",
        "node_modules/@prisma/**"
      ]
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/vercel.ts"
    }
  ]
}
{
  "version": 2,
  "builds": [
    {
      "src": "src/vercel.ts",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/vercel.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

